{
    "files.autoSave": "onFocusChange",
    "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Mono', 'Source Han Sans CN', '微软雅黑', Consolas, 'Courier New', monospace",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.titleBar.enabled": false,
    "editor.cursorBlinking": "solid",
    "terminal.integrated.sendKeybindingsToShell": true,
    "catppuccin.italicKeywords": false,
    "workbench.colorTheme": "Catppuccin Mocha",
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "workbench.iconTheme": "catppuccin-mocha",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "vim.neovimUseConfigFile": true,
    "vim.neovimConfigPath": "~/.confg/nvim",
    "vim.enableNeovim": true,
    "editor.accessibilityPageSize": 500,
    "window.titleBarStyle": "native",
    "workbench.activityBar.orientation": "vertical",
    "window.zoomPerWindow": false,
    "workbench.preferredHighContrastColorTheme": "Catppuccin Mocha",
    "workbench.layoutControl.enabled": false,
    "zenMode.hideStatusBar": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.fullScreen": false,
    "window.menuBarVisibility": "toggle",
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "editor.suggestOnTriggerCharacters": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.tabCompletion": "on",
    "editor.inlineSuggest.enabled": true,
    "editor.suggest.preview": true,
    "editor.hover.enabled": true,
    "editor.suggest.showInlineDetails": true,
    "editor.snippetSuggestions": "top",
    "cursor.editorCompletion.enableInlineCompletion": true,
    "cursor.editorCompletion.aiFillMode": "aggressive",
    "cursor.editorCompletion.enableEarlyHinting": true,
    "cursor.features.traceValues.enabled": true,
    "cursor.features.codeActions.enabled": true,
    "cursor.features.copilot.enabled": true,
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vscode-neovim.useCtrlKeysForNormalMode": false,
    "vscode-neovim.useCtrlKeysForInsertMode": false,
    "rust-analyzer.diagnostics.enable": false,
    "digital-ide.welcome.show": false,
    "digital-ide.function.lsp.linter.verilog.diagnostor": "verilator",
    "git.autofetch": true,
    "terminal.integrated.tabs.enabled": false,
    "cursor.general.gitGraphIndexing": "enabled",
    "cmake.showConfigureWithDebuggerNotification": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "cursor.general.disableHttp2": true,
    "window.zoomLevel": 2
}
